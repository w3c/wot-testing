 "base": "http://172.16.1.219/ur10/",
 "goTo": {
  "description": "Move to position in cartesian coordinates with the assigned speed and acceleration, default values are .... ",
  "idempotent": true,
  "input": {
   "properties": {
    "a": {
     "maximum": 1.0,
     "minimum": 0.1,
     "type": "number"
    },
    "s": {
     "maximum": 1.0,
     "minimum": 0.1,
     "type": "number"
    },
    "x": {
     "maximum": 1300,
     "minimum": -1300,
     "type": "integer"
    },
    "y": {
     "maximum": 1300,
     "minimum": -1300,
     "type": "integer"
    },
    "z": {
     "maximum": 1300,
     "minimum": -1300,
     "type": "integer"
    }
   },
   "type": "object"
  },
  "safe": false,
  "title": "Go to position"
 },
