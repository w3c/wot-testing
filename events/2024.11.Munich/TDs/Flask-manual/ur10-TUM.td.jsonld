    {
     "contentType": "application/json",
     "href": "properties/homePosition",
     "op": [
      "readproperty"
     ]
    }
   ],
   "properties": {
    "base": {
     "type": "number"
    },
    "elbow": {
     "type": "number"
    },
    "shoulder": {
     "type": "number"
    },
    "wrist1": {
     "type": "number"
    },
    "wrist2": {
     "type": "number"
    },
    "wrist3": {
     "type": "number"
    },
    "x": {
     "type": "integer"
    },
    "y": {
     "type": "integer"
    },
    "z": {
     "type": "integer"
    }
   },
   "readOnly": true,
   "required": [
    "x",
    "y",
    "z"
   ],
   "title": "Home position",
   "type": "object"
  },
